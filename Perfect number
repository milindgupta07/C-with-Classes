#include <iostream>
using namespace std;

int main() {
    int n, i = 1, sum = 0;
    cout << "Enter number: ";
    cin >> n;

    while(i < n) {
        if(n % i == 0)
            sum += i;
        i++;
    }

    if(sum == n)
        cout << "Perfect Number";
    else
        cout << "Not Perfect";

    return 0;
}
