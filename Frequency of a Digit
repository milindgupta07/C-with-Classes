#include <iostream>
using namespace std;

int main() {
    int num, digit, count = 0, search;
    cout << "Enter number: ";
    cin >> num;

    cout << "Enter digit to search: ";
    cin >> search;

    while(num > 0) {
        digit = num % 10;
        if(digit == search)
            count++;
        num /= 10;
    }

    cout << "Frequency = " << count;
    return 0;
}
