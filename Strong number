#include <iostream>
using namespace std;

int main() {
    int num, temp, sum = 0;
    cout << "Enter number: ";
    cin >> num;

    temp = num;

    while(temp > 0) {
        int digit = temp % 10;
        int fact = 1;
        int i = 1;

        while(i <= digit) {
            fact *= i;
            i++;
        }

        sum += fact;
        temp /= 10;
    }

    if(sum == num)
        cout << "Strong Number";
    else
        cout << "Not Strong";

    return 0;
}
